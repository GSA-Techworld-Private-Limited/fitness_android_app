<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listrectangle430RowModel" type="com.fitness.app.modules.sstone.data.model.Listrectangle430RowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowrectangle430"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageRectangle430"
            android:layout_height="@dimen/_115pxh"
            android:layout_width="@dimen/_136pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_10pxh}"
            imageRes="@{@drawable/img_rectangle430_115x136}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle430_115x136"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumntaskname"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_weight="0.54"
            android:layout_width="0dp"
            android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtTaskName"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{listrectangle430RowModel.txtTaskName}"
                style="@style/txtRalewayromanmedium12" tools:text="@string/lbl_task_name"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="match_parent"
                android:text="@{listrectangle430RowModel.txtDescription}"
                style="@style/txtRalewayromanregular10_1" tools:text="@string/msg_worem_ipsum_dol"/>
            <FrameLayout android:id="@+id/frameStackcompleted"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="@dimen/_98pxh">
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtCompleted"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{listrectangle430RowModel.txtCompleted}"
                    style="@style/txtRalewayromanregular10_2" tools:text="@string/lbl_completed"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnCompletedOne"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_23pxh"
                    android:paddingStart="@dimen/_22pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_completed"
                    android:textAllCaps="false"
                    style="@style/btnGradientRounded_1" tools:text="@string/lbl_completed"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
