                android:layout_height="@dimen/_115pxh"
                android:layout_width="@dimen/_136pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_10pxh}"
                imageRes="@{@drawable/img_rectangle430}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle430"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumndescriptionOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_weight="0.54" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescriptionOne"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="@{sstOneSixVM.sstOneSixModel.txtDescriptionOne}"
                    style="@style/txtRalewayromanregular10_1" tools:text="@string/msg_worem_ipsum_dol"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnAttendOne"
                    android:layout_height="@dimen/_30pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_98pxh"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_attend"
                    android:textAllCaps="false"
                    style="@style/btnGradientRounded_1" tools:text="@string/lbl_attend"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
