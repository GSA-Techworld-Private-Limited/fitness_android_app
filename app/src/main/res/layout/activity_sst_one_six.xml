                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPhaseCounter"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.39"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{sstOneSixVM.sstOneSixModel.txtPhaseCounter}"
                        style="@style/txtNunitosemibold10" tools:text="@string/lbl_phase_1"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPhaseCounterOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_weight="0.39"
                        android:layout_width="0dp"
