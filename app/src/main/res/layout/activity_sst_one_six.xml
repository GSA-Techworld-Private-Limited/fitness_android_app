                        android:id="@+id/txtDescription"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:text="@{sstOneSixVM.sstOneSixModel.txtDescription}"
                        style="@style/txtRalewayromanregular10_1" tools:text="@string/msg_worem_ipsum_dol"/>
