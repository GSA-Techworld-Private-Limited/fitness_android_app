            <ImageView android:id="@+id/imageGroup100000210"
                android:layout_height="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_group100000210_11x329"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group100000210_11x329"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtDayCounter"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_33pxv"
