                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPhaseCounterThree"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.39"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{sstOneSixVM.sstOneSixModel.txtPhaseCounterThree}"
                        style="@style/txtNunitosemibold10" tools:text="@string/lbl_phase_4"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPhaseCounterFour"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.39"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{sstOneSixVM.sstOneSixModel.txtPhaseCounterFour}"
                        style="@style/txtNunitosemibold10" tools:text="@string/lbl_phase_5"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPhaseCounterFive"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_weight="0.38"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{sstOneSixVM.sstOneSixModel.txtPhaseCounterFive}"
                        style="@style/txtNunitosemibold10" tools:text="@string/lbl_phase_6"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPhaseCounterSix"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.39"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{sstOneSixVM.sstOneSixModel.txtPhaseCounterSix}"
