<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable name="homeContainerVM" type="com.fitness.app.modules.homecontainer.data.viewmodel.HomeContainerVM"/>
    </data>

    <LinearLayout
        android:gravity="start"
        android:id="@+id/linearColumnwelcomebacks"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        style="@style/groupStylewhite_A700">

        <FrameLayout android:id="@+id/fragmentContainer"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_width="match_parent"/>

        <com.google.android.material.bottomnavigation.BottomNavigationView
            android:id="@+id/frameBottombar"
            style="@style/groupStylewhite_1"
            android:layout_width="match_parent"
            android:layout_height="65dp"
            app:itemIconTint="@drawable/icon_tinit_selector"
            app:itemTextColor="@drawable/icon_tinit_selector"
            app:menu="@menu/nav_menu"
            app:labelVisibilityMode="labeled"/>


    </LinearLayout>
</layout>
