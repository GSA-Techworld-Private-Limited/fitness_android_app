<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="plyometricsRowModel" type="com.fitness.app.modules.plyometrics.data.model.PlyometricsRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameStackrectangle448"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <ImageView android:id="@+id/imageRectangle448"
            android:layout_gravity="start"
            android:layout_height="@dimen/_178pxh"
            android:layout_width="@dimen/_320pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_10pxh}"
            imageRes="@{@drawable/img_rectangle448_2}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle448_2"/>
        <LinearLayout android:id="@+id/linearColumnplaycircle"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imagePlaycircle"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_30pxh"
                android:layout_marginEnd="@dimen/_112pxh"
                android:layout_marginStart="@dimen/_112pxh"
                android:layout_width="@dimen/_30pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_playcircle"
                tools:ignore="ContentDescription" tools:src="@drawable/img_playcircle"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPlyometricCounter"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_112pxh"
                android:layout_marginStart="@dimen/_112pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{plyometricsRowModel.txtPlyometricCounter}"
                style="@style/txtRobotoromanregular16_5" tools:text="@string/lbl_plyometric_1"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnComplete"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_13pxv"
                android:singleLine="true"
                android:text="@string/lbl_complete"
                android:textAllCaps="false"
                style="@style/btnGradientRoundedBottomCorner" tools:text="@string/lbl_complete"/>
        </LinearLayout>
    </FrameLayout>
</layout>
