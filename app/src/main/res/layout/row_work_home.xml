<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_gravity="start"
    android:layout_height="wrap_content">


    <LinearLayout
        android:id="@+id/linearColumnsstone"
        style="@style/groupStylecornerRadius"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20pxh"
        android:layout_marginTop="@dimen/_20pxv"
        android:layout_marginEnd="@dimen/_20pxh"
        android:layout_marginBottom="@dimen/_20pxv"
        android:gravity="start"
        android:orientation="vertical">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <TextView
            android:id="@+id/txtSSTOne"
            style="@style/txtRobotoromansemibold20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@string/lbl_sst_1"
            tools:text="@string/lbl_sst_1" />

            <LinearLayout
                android:layout_marginEnd="10dp"
                android:gravity="end"
                android:layout_gravity="bottom"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/dayname"
                    android:text="Day 1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/txtRobotoromansemibold1611"/>

                <ImageView
                    android:src="@drawable/img_arrowright_white_a700"
                    android:layout_width="32dp"
                    android:layout_height="32dp"/>
            </LinearLayout>
        </LinearLayout>
        <ProgressBar
            android:id="@+id/progressBarGroup100000199"
            style="@style/Widget.AppCompat.ProgressBar.Horizontal"
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:gravity="center_horizontal"
            android:indeterminate="false"
            android:max="100"
            android:minHeight="@dimen/_12pxv"
            android:progress="34"
            android:progressDrawable="@drawable/layer_list_bg_gray_603_radius_6_16" />


        <LinearLayout
            android:gravity="end"
            android:layout_gravity="start"
            android:layout_marginEnd="20dp"
            android:id="@+id/linearRowthree"
            android:layout_marginBottom="@dimen/_20pxh"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_width="wrap_content"
            android:orientation="horizontal">

            <LinearLayout

                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtThree"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="0"
                    style="@style/txtRobotoromanregular16_5"
                    tools:text="0"/>
                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtThree1"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="/"
                    style="@style/txtRobotoromanregular16_5"
                    tools:text="/"/>
                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtThree2"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="0"
                    style="@style/txtRobotoromanregular16_5"
                    tools:text="0"/>

            </LinearLayout>


            <TextView android:ellipsize="end"
                android:id="@+id/txtCompleted"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.76" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_completed"
                style="@style/txtRobotoromanregular16_5"
                tools:text="@string/lbl_completed"/>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>